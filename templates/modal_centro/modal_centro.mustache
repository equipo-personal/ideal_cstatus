        </div>
        <script src="{{{config.wwwroot}}}/blocks/ideal_cstatus/templates/modal_centro/confetti.js"></script>
        <button type="button" id="btn_close_modal" onclick="closeModal()">{{#str}}Close,block_ideal_cstatus{{/str}}</button>
