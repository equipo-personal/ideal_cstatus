                <th class="td_nivel">{{#str}}Level,block_ideal_cstatus{{/str}}</th>
                <th class="th_matriculado">{{#str}}registered,block_ideal_cstatus{{/str}}</th>
            </tr>
        </table>
        <div id="all_complete">
