<script>
    var learning_plans = {{{template_data}}};
    var lang_user_ = {{{lang_user}}};
    var str_completed = `{{#str}}Completed,block_ideal_cstatus{{/str}}`;
    var str_in_progress = `{{#str}}In_Progress,block_ideal_cstatus{{/str}}`;
    var str_registered = `{{#str}}registered,block_ideal_cstatus{{/str}}`;
    var str_not_registered = `{{#str}}not_registered,block_ideal_cstatus{{/str}}`;
    var str_not_learningP = `{{#str}}not_learning_plan_label,block_ideal_cstatus{{/str}}`;
