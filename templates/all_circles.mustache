    <script>
    //Cargar los estilos de manera dinamica
        window.onload = function() {
            var head = document.getElementsByTagName('head')[0]; 
            if (head) {
                var link = document.createElement('link');
                link.rel = 'stylesheet';
                link.type = 'text/css';
                link.href = '../blocks/ideal_cstatus/amd/build/styles.min.css';
                head.appendChild(link); 
            }
        };
    </script>
</head>
<div class="container_main" id='container_main'>
    <div class="section_1">
        <div class="container_children">
            {{#circles}}
                {{#circle1}}
